<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#FF000000</color>
    <color name="on_surface_dark">#FFFFFFFF</color>
    <color name="on_surface_variant_dark">#FFCCCCCC</color>
    <color name="outline_dark">#FF444444</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="surface_dark">#FF121212</color>
    <color name="surface_variant_dark">#FF1E1E1E</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <color name="widget_background_dark">#FF2D2D2D</color>
    <color name="widget_icon_dark">#FFBBBBBB</color>
    <color name="widget_text_dark">#FFCCCCCC</color>
    <style name="Theme.SearchBox" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        
        
        <item name="colorSurface">@color/surface_dark</item>
        <item name="colorOnSurface">@color/on_surface_dark</item>
        <item name="colorSurfaceVariant">@color/surface_variant_dark</item>
        <item name="colorOnSurfaceVariant">@color/on_surface_variant_dark</item>
        <item name="colorOutline">@color/outline_dark</item>
        
        
        <item name="android:statusBarColor">@color/surface_dark</item>
        <item name="android:windowLightStatusBar" ns1:targetApi="m">false</item>
        
        
        <item name="android:windowBackground">@color/surface_dark</item>
    </style>
    <style name="Theme.SearchBox.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar" ns1:targetApi="m">false</item>
        <item name="android:statusBarColor">@color/surface_dark</item>
        <item name="android:windowBackground">@color/surface_dark</item>
    </style>
</resources>